{
  "expo": {
    "name": "PAZ",
    "slug": "paz",
    "scheme": "upaz",
    "version": "1.2.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0B0B0F",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0F"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0B0B0F",
      "translucent": false
    },
    "android": {
      "package": "com.jhoonero.paz",
       "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0B0F"
      },
      "splash": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0B0B0F"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "upaz.com",
              "pathPrefix": "/auth/callback"
            },
            {
              "scheme": "upaz"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.jhoonero.paz",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0B0B0F"
      },
      "supportsTablet": true,
      "associatedDomains": ["applinks:upaz.com"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2473071f-0b00-458a-8501-8959ea99c1b9"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
